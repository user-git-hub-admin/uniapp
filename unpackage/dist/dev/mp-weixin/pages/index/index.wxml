<view class="index"><view class="indexHead"><view class="date">{{day}}<text>{{month}}</text></view><view class="daily hours">{{hours}}</view><view data-event-opts="{{[['tap',[['tiao']]]]}}" class="homePage" bindtap="__e"><image src="../../static/images/tuoyuan.png"></image></view></view><view class="box"><view class="indexBanner"><swiper autoplay interval="2000" indicator-dots indicator-color="#666" indicator-active-color="#ccc"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image src="{{item.image}}"></image><view>{{item.title}}</view></swiper-item></block></swiper></view></view><view class="listDl"><block wx:for="{{news}}" wx:for-item="n" wx:for-index="dls" wx:key="dls"><view data-event-opts="{{[['tap',[['bot']]]]}}" bindtap="__e" class="_dl"><view class="_dt"><image src="{{n.images}}"></image></view><view class="_dd">{{n.title}}</view><view class="read _dd">{{n.hint}}</view></view></block></view></view>